package fr.bestdevelop.launcher.sse.ui.contents;

import java.awt.Graphics;
import java.awt.Image;

import javax.swing.JPanel;

import fr.bestdevelop.launcher.sse.utils.Utils;

public class LauncherContent extends JPanel{
	
	public LauncherContent() {
		this.setLayout(null);
		setupPanelComponent();
	}
	
	private void setupPanelComponent() {
		
	}
	
	public void paintComponent(Graphics g) {
		super.paintComponent(g);
		
		Image background = Utils.getResource("contents/test.png");
		g.drawImage(background, 0, 0, this.getWidth(), this.getHeight(), this);
	}
}
